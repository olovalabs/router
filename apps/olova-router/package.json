{
  "name": "olova-router",
  "version": "1.0.6",
  "description": "A client-first folder-based routing for React + Vite applications",
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./router": {
      "import": "./dist/router.js",
      "types": "./dist/router.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "javascript",
    "url",
    "search",
    "framework",
    "typescript",
    "router",
    "state-management",
    "ssr",
    "ssg",
    "routing",
    "rpc",
    "route",
    "typesafe",
    "fullstack",
    "searchparams",
    "server-functions",
    "vite",
    "vite-plugin",
    "file-based-routing",
    "folder-routing",
    "client-first"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "vite": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
